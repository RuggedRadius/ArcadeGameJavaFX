<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="800.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <SplitPane dividerPositions="0.6526610644257703" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.valignment="TOP" GridPane.vgrow="ALWAYS">
            <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <HBox id="gameFrame" fx:id="gameFrame" prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
                </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                    <children>
                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <children>
                                <Label text="High Scores" />
                                <ListView prefHeight="200.0" prefWidth="200.0" />
                                <Separator prefWidth="200.0" />
                                <Label text="Playlist" />
                        <VBox>
                           <children>
                              <ListView fx:id="lstPlaylist" prefHeight="200.0" prefWidth="200.0" />
                              <HBox alignment="CENTER">
                                 <children>
                                    <Button fx:id="btnPlay" mnemonicParsing="false" onAction="#musicPlay" text="►">
                                       <font>
                                          <Font name="Stencil" size="19.0" />
                                       </font>
                                    </Button>
                                    <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#musicStop" text="■">
                                       <font>
                                          <Font name="Stencil" size="19.0" />
                                       </font>
                                    </Button>
                                    <Button layoutX="62.0" layoutY="10.0" mnemonicParsing="false" onAction="#musicPrevious" text="|◄◄">
                                       <font>
                                          <Font name="Stencil" size="19.0" />
                                       </font>
                                    </Button>
                                    <Button layoutX="114.0" layoutY="10.0" mnemonicParsing="false" onAction="#musicNext" text="►►|">
                                       <font>
                                          <Font name="Stencil" size="19.0" />
                                       </font>
                                    </Button>
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                            </children></VBox>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </center>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <bottom>
      <HBox id="scoreBoard" prefWidth="646.0" spacing="100.0" style="-fx-background-color: #000;" stylesheets="@styles/styles.css" BorderPane.alignment="CENTER">
         <children>
            <HBox HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Score: " textAlignment="CENTER" textFill="WHITE">
                     <font>
                        <Font name="OCR A Extended" size="30.0" />
                     </font>
                  </Label>
                  <Label id="lblScore" fx:id="lblScore" text="0" textAlignment="CENTER" textFill="WHITE">
                     <font>
                        <Font name="OCR A Extended" size="30.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="10.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Level: " textAlignment="CENTER" textFill="WHITE">
                     <font>
                        <Font name="OCR A Extended" size="30.0" />
                     </font>
                  </Label>
                  <Label text="0" textAlignment="CENTER" textFill="WHITE">
                     <font>
                        <Font name="OCR A Extended" size="30.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
      </HBox>
   </bottom>
    </BorderPane>
